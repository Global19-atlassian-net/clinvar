            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="XRef" type="typeXref" minOccurs="0" maxOccurs="unbounded"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="Comment" type="typeComment" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element name="MolecularConsequenceList" minOccurs="0">
            <xs:annotation>
